{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
            "includeFiles": ["src/**/*.js"]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/.*",
            "dest": "src/index.js"
        }
    ]
}